
from sys import stdin


def removeAllOccurrencesOfChar(string, ch) :
	# Your code goes her
	new_str= ''
	for i in range(len(string)):
		if string[i]!=ch:
			new_str+=string[i]
	return new_str
			

	

#main
string = stdin.readline().strip()
# string = input().split()
ch = stdin.readline().strip()[0]
# ch= input().split()
ans = removeAllOccurrencesOfChar(string, ch)

print(ans)
